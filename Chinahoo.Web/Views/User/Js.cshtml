@{
    ViewBag.Title = "";
    var F = Html.F();
}


@section body {

    @(F.Panel().IsFluid(true).CssClass("blockpanel").ID("Panel2").ShowBorder(false).Layout(LayoutType.VBox).BoxConfigChildMargin("0 0 5 0").ShowHeader(false).TitleAlign(TextAlign.Center).Title("AES 操作")
        .Items(
            F.Panel().ID("Panel3").Title("js格式化").BodyPadding(10).ShowBorder(true).ShowHeader(true)
                        .Toolbars(
            F.Toolbar().Position(ToolbarPosition.Bottom).ToolbarAlign(ToolbarAlign.Center).ID("Toolbar1")
                .Items(
                   F.Button().ID("Button2").Text("格式化").ValidateTarget(Target.Top).ValidateForms("Panel2").OnClick(Url.Action("Encryptionjs_Click"),"Panel2")
                  
                )
        )
                .Items(
                   F.TextArea().Required(true).Height(500).ID("original").Label("内容")
                            
                )
           
        )
    )

}
